{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "types": ["node"],
    "sourceMap": true,
    "target": "ESNext",
    "module": "ESNext",
    "allowJs": true,
    "skipLibCheck": true,
    "experimentalDecorators": true,
    "skipDefaultLibCheck": true,
    "esModuleInterop": true,
    "moduleResolution": "Node",
    "resolveJsonModule": true,
    "strict": true,
    "jsx": "preserve",
    "jsxImportSource": "solid-js",
  }
}